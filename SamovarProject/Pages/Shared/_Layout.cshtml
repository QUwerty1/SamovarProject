@{
    dynamic text;
    var pageList = new Dictionary<dynamic, dynamic>()
    {
        {"Кубик", "/1boxPreview"},
        {"Дыра в пространстве", "/2holePreview"},
        {"Сложная модель", "/3complexModelPreview"},
        {"Аудио", "/4audioPreviw"},
        {"Видео", "/5videoPreview"},
        {"Кастомный маркер", "/6samovarMarkerPreview"},
        {"Много маркеров", "/7multiMarkerPreview"}

    };
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SamovarProject</title>
    <script src="https://aframe.io/releases/1.3.0/aframe.min.js"></script>
    <script src="https://raw.githack.com/AR-js-org/AR.js/master/aframe/build/aframe-ar.js"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/SamovarProject.styles.css" />
    @ViewData["Links"]

</head>
<body>

    <div class="mainContainer">
        <div class="toolbar">
            <div class="brand">
                <img class="logo" src="https://cdn.glitch.global/fb3e4ace-8100-4365-8fdc-9e2c2c51db5b/Samovar.png" />
                SamovarЫ
            </div>
            <div class="pages">
                @foreach (var i in pageList.Keys)
                {
                    <a class="pageLink" href="@pageList[i]">
                        <div class="pageLink">@i </div>
                    </a>
                }               
            </div>
        </div>
        <div class="renderBody">
            <main role="main" class="pb-3">
                <div class="code">
                    @RenderBody()
                </div>
                <div class="codeSource">
                    <a href='@ViewData["pageLink"]'>Ссылка на источник</a>
                </div>
                
            </main>
        </div>
        
    </div>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

</body>
</html>